<UserControl x:Class="BAWGUI.Results.Views.ResultsTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:views="clr-namespace:BAWGUI.Results.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <TabControl>
        <TabItem Header="Forced Oscillations">
            <views:ForcedOscillationResultsView DataContext="{Binding ForcedOscillationResultsViewModel}" />
        </TabItem>
        <TabItem Header="Out of Range Events">
            <views:OutOfRangeResultsView DataContext="{Binding OutOfRangeResultsViewModel}"></views:OutOfRangeResultsView>
        </TabItem>
        <TabItem Header="Ringdowns">
            <views:RingdownResultsView DataContext="{Binding RingdownResultsViewModel}"></views:RingdownResultsView>
        </TabItem>
        <TabItem Header="Wind Ramps">
            <views:WindRampResultsView DataContext="{Binding WindRampResultsViewModel}"></views:WindRampResultsView>
        </TabItem>
    </TabControl>
</UserControl>
