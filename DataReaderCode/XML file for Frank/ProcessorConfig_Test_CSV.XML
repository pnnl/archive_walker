<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Processing>
        <!--
        For interpolation, filtering, and sampling-rate conversion, if the 
        PMU element is omitted the process is applied to all signals in all
        PMUs. If the Channels element within a PMU element is omitted the 
        process is applied to all signals in that PMU. Otherwise, the 
        process is applied to the specific PMUs and signals listed in the
        elements.
        If channels are not specified for wrap and unwrap operations, they
        are applied to all angle signals in the PMUs.
        -->
        <SecondsToConcat>1240</SecondsToConcat>    
        <Unwrap>
            <PMU>
                <Name>Custom Signals</Name>
            </PMU>              
        </Unwrap>
        <Interpolate>
            <Parameters>
                <Limit>100</Limit>
                <Type>Linear</Type>
                <FlagInterp>TRUE</FlagInterp>
            </Parameters>            
        </Interpolate>
        <Stages>
            <Filter>
                <Type>Rational</Type>
                <Parameters>
                    <Numerator>1,-1</Numerator>
                    <Denominator>1</Denominator>
                    <ZeroPhase>TRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU> 
            </Filter>
            <Filter>
                <Type>HighPass</Type>
                <Parameters>
                    <Order>1</Order>
                    <Cutoff>0.1</Cutoff>
                    <ZeroPhase>TRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU>                
            </Filter>
            <Filter>
                <Type>LowPass</Type>
                <Parameters>
                    <PassRipple>0.1</PassRipple>
                    <StopRipple>50</StopRipple>
                    <PassCutoff>1.5</PassCutoff>
                    <StopCutoff>2.5</StopCutoff>
                    <ZeroPhase>TRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU> 
            </Filter>            
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals2</MultiRatePMU>
                    <NewRate>5</NewRate>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU> 
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals3</MultiRatePMU>
                    <p>2</p>
                    <q>24</q>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU>
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals4</MultiRatePMU>
                    <q>12</q>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU>
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals5</MultiRatePMU>
                    <NewRate>120</NewRate>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU> 
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals6</MultiRatePMU>
                    <p>2</p>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                </PMU>
            </Multirate>
        </Stages>            
        <Wrap>
            <PMU>
                <Name>Custom Signals</Name>
            </PMU>
        </Wrap>
    </Processing>     
    <!--
    Specify the signals for output
    -->
    <SignalSelection>
        <PMU>
            <Name>Custom Signals</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals2</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals3</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals4</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals5</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals6</Name>
        </PMU>
    </SignalSelection>
</Configuration>