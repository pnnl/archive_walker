    <!-- 
    Each stage is composed of data quality filters and customizations. 
    Filters are always applied first, then the customizations. The 
    stages are carried out in sequential order. 
    
    For all filters, if PMU isn't included, the filter is applied to all 
    PMUs. Under PMU, if Channel isn't included, the filter is applied for
    all appropriate channels in the specified PMUs. 
    -->
    <!--
    *******
    Stage 1
    *******
    -->
    <Stages>
        <!--
        **********************
        Stage 1 - DQ Filtering
        **********************
        -->
        <Filter>
            <Name>PMUflagFilt</Name>
            <!-- 
            This one can only be applied to entire PMUs, not individual 
            channels.
            -->
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>1</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DropOutZeroFilt</Name>
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>2</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DropOutMissingFilt</Name>
            <!-- 
            This one can only be applied to entire PMUs, not individual 
            channels.
            -->
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>3</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>VoltPhasorFilt</Name>
            <Parameters>
                <VoltMin>0.7</VoltMin>
                <VoltMax>1.3</VoltMax>
                <SetToNaN>FALSE</SetToNaN>
                <FlagBit>4</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>FreqFilt</Name>
            <!-- 
            This one is always applied to the frequency channels in a PMU, 
            so channels are not specified.
            -->
            <Parameters>
                <FreqMinChan>59.9</FreqMinChan>
                <FreqMaxChan>60.1</FreqMaxChan>
                <FreqPctChan>45</FreqPctChan>
                <FreqMinSamp>59</FreqMinSamp>
                <FreqMaxSamp>61</FreqMaxSamp>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBitChan>5</FlagBitChan>
                <FlagBitSamp>6</FlagBitSamp>
            </Parameters>
        </Filter>
        <Filter>
            <Name>OutlierFilt</Name>
            <!-- 
            This is an outlier filter, detection is dependednt on std. deviation value, which is to be provided by user.
            -->
            <Parameters>
                <StdDevMult>10</StdDevMult>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>7</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>StaleFilt</Name>
            <Parameters>
                <FlagAllByFreq>FALSE</FlagAllByFreq>
                <StaleThresh>60</StaleThresh>
                <SetToNaN>False</SetToNaN>
                <FlagBit>8</FlagBit>
                <FlagBitFreq>9</FlagBitFreq>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DataFrameFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>10</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>PMUchanFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>11</FlagBit>
            </Parameters>
        </Filter>
        <Filter>
            <Name>PMUallFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagBit>12</FlagBit>
            </Parameters>
        </Filter>
        <!--
        ***********************
        Stage 1 - Customization
        ***********************
        -->
        <Customization>
            <Name>ScalarRep</Name>
            <Parameters>
                <scalar>340</scalar>
                <SignalName>ScalarSig.M500</SignalName>
                <SignalType>VMP</SignalType>
                <SignalUnit>kV</SignalUnit>
            </Parameters>
        </Customization>
        <Customization>
            <Name>ScalarRep</Name>
            <Parameters>
                <scalar>7</scalar>
                <SignalName>ScalarSig1.M500</SignalName>
                <SignalType>VMP</SignalType>
                <SignalUnit>kV</SignalUnit>
            </Parameters>
        </Customization>
        <Customization>
            <Name>ScalarRep</Name>
            <Parameters>
                <scalar>60</scalar>
                <SignalName>Freq_CusSig</SignalName>
                <SignalType>F</SignalType>
                <SignalUnit>Hz</SignalUnit>
            </Parameters>
        </Customization>
    </Stages>
</Configuration>
		