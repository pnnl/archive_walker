<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
	<ReaderProperties>
		<FileDirectory>C:\Users\foll154\Documents\BPA Oscillation App\Learn XML\TestData</FileDirectory>
        <Mnemonic>WISPDitt</Mnemonic>
        <Mode>
            <Name>Hybrid</Name>
            <Params>
                <DateTimeStart>2015-10-17 12:30:00 GMT</DateTimeStart>
                <NoFutureWait>60</NoFutureWait>
                <MaxNoFutureCount>10</MaxNoFutureCount>
                <FutureWait>60</FutureWait>
                <MaxFutureCount>10</MaxFutureCount>
                <RealTimeRange>60</RealTimeRange>
            </Params>
        </Mode>
	</ReaderProperties>
	<SignalSelection>
		<PMU>
			<Name>ALSN 500 A1 SA  </Name>
			<Channels>
				<Channel>
					<Name>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Name>
				</Channel>
                <Channel>
					<Name>ALSN 500 A1 SA  .B500NORTH____1VP.ANG</Name>
				</Channel>
                <Channel>
					<Name>ALSN 500 A1 SA  .T500BANK1____1IP.MAG</Name>
				</Channel>
                <Channel>
					<Name>ALSN 500 A1 SA  .A500NAPAVINE_1MW</Name>
				</Channel>
			</Channels>
		</PMU>
		<PMU>
			<Name>JDAY 500  A1 SA </Name>
			<Channels>
				<Channel>
					<Name>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Name>
				</Channel>
                <Channel>
					<Name>JDAY 500  A1 SA .B500EAST_____1VP.ANG</Name>
				</Channel>
                <Channel>
					<Name>JDAY 500  A1 SA .B500EAST_____1VA.MAG</Name>
				</Channel>
				<Channel>
					<Name>JDAY 500  A1 SA .A500SLATT____1MW</Name>
				</Channel>
			</Channels>
		</PMU>
	</SignalSelection>
    <!-- 
    Each stage is composed of data quality filters and customizations. 
    Filters are always applied first, then the customizations. The 
    stages are carried out in sequential order. 
    
    For all filters, if PMU isn't included, the filter is applied to all 
    PMUs. Under PMU, if Channel isn't included, the filter is applied for
    all appropriate channels in the specified PMUs. 
    -->
    <!--
    *******
    Stage 1
    *******
    -->
    <Stages>
        <!--
        **********************
        Stage 1 - DQ Filtering
        **********************
        -->
        <Filter>
            <Name>PMUflagFilt</Name>
            <!-- 
            This one can only be applied to entire PMUs, not individual 
            channels.
            -->
            <PMU>
                <Name>ALSN 500 A1 SA  </Name>
            </PMU>
            <PMU>
                <Name>JDAY 500  A1 SA </Name>
            </PMU>
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>1</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DropOutZeroFilt</Name>
            <PMU>
                <Name>ALSN 500 A1 SA  </Name>
                <Channel>
					<Name>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Name>
				</Channel>
            </PMU>
            <PMU>
                <Name>JDAY 500  A1 SA </Name>
                <Channel>
					<Name>JDAY 500  A1 SA .B500EAST_____1VP.ANG</Name>
				</Channel>
				<Channel>
					<Name>JDAY 500  A1 SA .A500SLATT____1MW</Name>
				</Channel>
            </PMU>
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>2</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DropOutMissingFilt</Name>
            <!-- 
            This one can only be applied to entire PMUs, not individual 
            channels.
            -->
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>3</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>VoltPhasorFilt</Name>
            <Parameters>
                <VoltMin>0.7</VoltMin>
                <VoltMax>1.3</VoltMax>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>4</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>FreqFilt</Name>
            <!-- 
            This one is always applied to the frequency channels in a PMU, 
            so channels are not specified.
            -->
            <Parameters>
                <FreqMinChan>59.9</FreqMinChan>
                <FreqMaxChan>60.1</FreqMaxChan>
                <FreqPctChan>45</FreqPctChan>
                <FreqMinSamp>59</FreqMinSamp>
                <FreqMaxSamp>61</FreqMaxSamp>
                <SetToNaN>TRUE</SetToNaN>
                <FlagValChan>5</FlagValChan>
                <FlagValSamp>6</FlagValSamp>
            </Parameters>
        </Filter>
        <Filter>
            <Name>StaleFilt</Name>
            <PMU>
                <Name>ALSN 500 A1 SA  </Name>
            </PMU>
            <Parameters>
                <FlagAllByFreq>TRUE</FlagAllByFreq>
                <StaleThresh>60</StaleThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>7</FlagVal>
                <FlagValFreq>8</FlagValFreq>
            </Parameters>
        </Filter>
        <Filter>
            <Name>DataFrameFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>9</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>PMUchanFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>10</FlagVal>
            </Parameters>
        </Filter>
        <Filter>
            <Name>PMUallFilt</Name>
            <Parameters>
                <PercentBadThresh>25</PercentBadThresh>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>11</FlagVal>
            </Parameters>
        </Filter>
        <!--
        ***********************
        Stage 1 - Customization
        ***********************
        -->
        <Customization>
            <Name>ScalarRep</Name>
            <Parameters>
                <scalar>7</scalar>
                <SignalName>ScalarSig</SignalName>
                <SignalType>VMP</SignalType>
                <SignalUnit>kV</SignalUnit>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Addition</Name>
            <Parameters>
                <SignalName>Add3sigs</SignalName>
                <FlagVal>12</FlagVal>
                <term>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                </term>
                <term>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                </term>
                <term>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VA.MAG</Channel>
                </term>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Subtraction</Name>
            <Parameters>
                <SignalName>SubtractionSig</SignalName>
                <FlagVal>13</FlagVal>
                <minuend>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                </minuend>
                <subtrahend>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                </subtrahend>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Multiplication</Name>
            <Parameters>
                <SignalName>Mult3sigs</SignalName>
                <FlagVal>14</FlagVal>
                <factor>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                </factor>
                <factor>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                </factor>
                <factor>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VA.MAG</Channel>
                </factor>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Division</Name>
            <Parameters>
                <SignalName>DivisionSig</SignalName>
                <FlagVal>15</FlagVal>
                <dividend>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                </dividend>
                <divisor>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                </divisor>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Exponent</Name>
            <Parameters>
                <exponent>2</exponent>
                <FlagVal>16</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1ToExp</CustName>
                </signal>
                <signal>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                    <CustName>Sig2ToExp</CustName>
                </signal>
                <signal>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VA.MAG</Channel>
                    <CustName>Sig3ToExp</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig4ToExp</CustName>
                </signal>
            </Parameters>
        </Customization>
        <!--
        Note that SignReversal, AbsVal, RealComponent, ImagComponent, and 
        ComplexConj are all implemented with the same function.
        -->
        <Customization>
            <Name>SignReversal</Name>
            <Parameters>
                <FlagVal>17</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1SignRev</CustName>
                </signal>
                <signal>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .B500EAST_____1VP.ANG</Channel>
                    <CustName>Sig2SignRev</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3SignRev</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>AbsVal</Name>
            <Parameters>
                <FlagVal>18</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1SignRev</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3SignRev</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>RealComponent</Name>
            <Parameters>
                <FlagVal>19</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1SignRev</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3SignRev</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>ImagComponent</Name>
            <Parameters>
                <FlagVal>20</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1SignRev</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3SignRev</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>ComplexConj</Name>
            <Parameters>
                <FlagVal>21</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1SignRev</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3SignRev</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>Angle</Name>
            <Parameters>
                <FlagVal>22</FlagVal>
                <signal>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <CustName>Sig1Angle</CustName>
                </signal>
                <signal>
                    <PMU>Custom Signals</PMU>
                    <Channel>DivisionSig</Channel>
                    <CustName>Sig3Angle</CustName>
                </signal>
            </Parameters>
        </Customization>
        <Customization>
            <Name>CreatePhasor</Name>
            <Parameters>
                <FlagVal>23</FlagVal>
                <phasor>
                    <mag>
                        <PMU>ALSN 500 A1 SA  </PMU>
                        <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    </mag>
                    <ang>
                        <PMU>ALSN 500 A1 SA  </PMU>
                        <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.ANG</Channel>
                    </ang>
                    <CustName>Sig1Phasor</CustName>
                </phasor>
                <phasor>
                    <mag>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .B500EAST_____1VP.MAG</Channel>
                    </mag>
                    <ang>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .B500EAST_____1VP.ANG</Channel>
                    </ang>
                    <CustName>VoltagePhasor</CustName>
                </phasor>
                <phasor>
                    <mag>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .L500SLATT____1IP.MAG</Channel>
                    </mag>
                    <ang>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .L500SLATT____1IP.ANG</Channel>
                    </ang>
                    <CustName>CurrentPhasor</CustName>
                </phasor>
            </Parameters>
        </Customization>
        <Customization>
            <Name>PowerCalc</Name>
            <Parameters>
                <FlagVal>24</FlagVal>
                <!-- Options are Complex, Apparent, Active, and Reactive -->
                <PowType>Complex</PowType>
                <power>
                    <Vmag>
                        <PMU>ALSN 500 A1 SA  </PMU>
                        <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    </Vmag>
                    <Vang>
                        <PMU>ALSN 500 A1 SA  </PMU>
                        <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.ANG</Channel>
                    </Vang>
                    <Imag>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .L500SLATT____1IP.MAG</Channel>
                    </Imag>
                    <Iang>
                        <PMU>JDAY 500  A1 SA </PMU>
                        <Channel>JDAY 500  A1 SA .L500SLATT____1IP.ANG</Channel>
                    </Iang>
                    <CustName>Sig1Power</CustName>
                </power>
                <power>
                    <Vphasor>
                        <PMU>Custom Signals</PMU>
                        <Channel>VoltagePhasor</Channel>
                    </Vphasor>
                    <Iphasor>
                        <PMU>Custom Signals</PMU>
                        <Channel>CurrentPhasor</Channel>
                    </Iphasor>
                    <CustName>Sig2Phasor</CustName>
                </power>
            </Parameters>
        </Customization>
        <Customization>
            <Name>SpecTypeUnit</Name>
            <Parameters>
                <SigType>IMP</SigType>
                <SigUnit>A</SigUnit>
                <PMU>Custom Signals</PMU>
                <Channel>Add3sigs</Channel>
                <!-- CustName is optional. If excluded, replaces input signal -->
                <CustName>Sig2TypeUnit</CustName>
            </Parameters>
        </Customization>
        <Customization>
            <Name>MetricPrefix</Name>
            <Parameters>
                <ToConvert>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.MAG</Channel>
                    <NewUnit>V</NewUnit>
                    <!-- CustName is optional. If excluded, replaces input signal -->
                    <CustName>Sig2metric</CustName>
                </ToConvert>
                <ToConvert>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .L500SLATT____1IP.MAG</Channel>
                    <NewUnit>kA</NewUnit>
                </ToConvert>
            </Parameters>
        </Customization>
        <Customization>
            <Name>AngleConversion</Name>
            <Parameters>
                <ToConvert>
                    <PMU>ALSN 500 A1 SA  </PMU>
                    <Channel>ALSN 500 A1 SA  .B500NORTH____1VP.ANG</Channel>
                    <!-- CustName is optional. If excluded, replaces input signal -->
                    <CustName>Sig2angConv</CustName>
                </ToConvert>
                <ToConvert>
                    <PMU>JDAY 500  A1 SA </PMU>
                    <Channel>JDAY 500  A1 SA .L500SLATT____1IP.ANG</Channel>
                </ToConvert>
            </Parameters>
        </Customization>
    </Stages>
    <!--
    *******
    Stage 2
    *******
    -->
    <Stages>
        <!--
        **********************
        Stage 2 - DQ Filtering
        **********************
        -->
        <Filter>
            <Name>PMUflagFilt</Name>
            <!-- 
            This one can only be applied to entire PMUs, not individual 
            channels.
            -->
            <PMU>
                <Name>ALSN 500 A1 SA  </Name>
            </PMU>
            <PMU>
                <Name>JDAY 500  A1 SA </Name>
            </PMU>
            <Parameters>
                <SetToNaN>TRUE</SetToNaN>
                <FlagVal>1</FlagVal>
            </Parameters>
        </Filter>
        <!--
        ***********************
        Stage 2 - Customization
        ***********************
        -->
        <Customization>
        </Customization>
    </Stages>
</Configuration>
		