<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Processing>
        <!--
        For interpolation, filtering, and sampling-rate conversion, if the 
        PMU element is omitted the process is applied to all signals in all
        PMUs. If the Channels element within a PMU element is omitted the 
        process is applied to all signals in that PMU. Otherwise, the 
        process is applied to the specific PMUs and signals listed in the
        elements.
        If channels are not specified for wrap and unwrap operations, they
        are applied to all angle signals in the PMUs.
        -->
        <SecondsToConcat>1080</SecondsToConcat>    
        <Unwrap>
            <MaxNaN>1</MaxNaN>
        </Unwrap>
        <Interpolate>
            <Parameters>
                <Limit>100</Limit>
                <Type>Linear</Type>
                <FlagInterp>TRUE</FlagInterp>
            </Parameters>          
        </Interpolate>
        <Stages>
            <Filter>
                <Type>Rational</Type>
                <Parameters>
                    <Numerator>1,-1</Numerator>
                    <Denominator>1</Denominator>
                    <ZeroPhase>FALSE</ZeroPhase>
                </Parameters>
            </Filter>
            <Filter>
                <Type>HighPass</Type>
                <Parameters>
                    <Order>1</Order>
                    <Cutoff>0.1</Cutoff>
                    <ZeroPhase>FALSE</ZeroPhase>
                </Parameters>
            </Filter>
            <Filter>
                <Type>LowPass</Type>
                <Parameters>
                    <PassRipple>0.1</PassRipple>
                    <StopRipple>50</StopRipple>
                    <PassCutoff>1.5</PassCutoff>
                    <StopCutoff>2.5</StopCutoff>
                    <ZeroPhase>FALSE</ZeroPhase>
                </Parameters>
            </Filter>
        </Stages>
    </Processing>   
    <NameTypeUnit>
        <NewUnit>Hz</NewUnit>
        <NewType>F</NewType>
        <!--
        <PMU>
            <Name>Custom Signals</Name>
            <CurrentChannel>MALN</CurrentChannel>
            <NewChannel>MALN</NewChannel>
            <NewUnit>Hz</NewUnit>
            <NewType>F</NewType>
        </PMU>
        <PMU>
            <Name>Custom Signals</Name>
            <CurrentChannel>JDAY</CurrentChannel>
            <NewChannel>JDAY</NewChannel>
            <NewUnit>Hz</NewUnit>
            <NewType>F</NewType>
        </PMU>
        -->
    </NameTypeUnit>
    <!--
    Specify the signals for output
    -->
    <SignalSelection>
        <PMU>
            <Name>ModeMeterSignals</Name>
            <Channels>
            	<Channel>
                	<Name>MALIN500.VPA</Name>
            	</Channel>
            	<Channel>
                	<Name>CJK500.VPA</Name>
            	</Channel>
            	<Channel>
                	<Name>JDA500.VPA</Name>
            	</Channel>
                <Channel>
                	<Name>BIGED500.VPA</Name>
            	</Channel>
                <Channel>
                	<Name>MONROE500.VPA</Name>
            	</Channel>
                <Channel>
                	<Name>CUSTER500.VPA</Name>
            	</Channel>
                <Channel>
                	<Name>GARRISON500.VPA</Name>
            	</Channel>
                <Channel>
                	<Name>CHJ500.VPA</Name>
            	</Channel>
            </Channels>
        </PMU>
    </SignalSelection>
</Configuration>