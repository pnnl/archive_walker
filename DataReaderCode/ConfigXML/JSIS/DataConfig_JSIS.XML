    </Stages>
    <SignalSelection>
		<PMU>
			<Name>Custom Signals</Name>
			<Channels>
				<Channel>
					<Name>MALN</Name>
				</Channel>
                <Channel>
					<Name>JDAY</Name>
				</Channel>
			</Channels>
		</PMU>
