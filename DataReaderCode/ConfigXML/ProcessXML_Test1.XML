<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Processing>
        <!--
        For interpolation, filtering, and sampling-rate conversion, if the 
        PMU element is omitted the process is applied to all signals in all
        PMUs. If the Channels element within a PMU element is omitted the 
        process is applied to all signals in that PMU. Otherwise, the 
        process is applied to the specific PMUs and signals listed in the
        elements.
        If channels are not specified for wrap and unwrap operations, they
        are applied to all angle signals in the PMUs.
        -->
        <SecondsToConcat>300</SecondsToConcat>    
        <Unwrap>
            <PMU>
                <Name>Custom Signals</Name>
                 <Channel>
                    <Name>ScaledAngleDiffSignal2</Name>
                </Channel>
                <Channel>
                    <Name>ScaledAngleDiffSignal1</Name>
                </Channel>
            </PMU>              
        </Unwrap>
        <Interpolate>
            <Parameters>
                <Limit>100</Limit>
                <Type>Constant</Type>
                <FlagInterp>TRUE</FlagInterp>
            </Parameters>    
            <PMU>
                <Name>Custom Signals</Name>
                <Channel>
                    <Name>ScaledAngleDiffSignal1</Name>
                </Channel>
            </PMU>        
        </Interpolate>
        <Interpolate>
            <Parameters>
                <Limit>100</Limit>
                <Type>Linear</Type>
                <FlagInterp>TRUE</FlagInterp>
            </Parameters>
        </Interpolate>
        <Stages>
            <Filter>
                <Type>Rational</Type>
                <Parameters>
                    <Numerator>1,-1</Numerator>
                    <Denominator>1</Denominator>
                    <ZeroPhase>nTRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU> 
            </Filter>
            <Filter>
                <Type>HighPass</Type>
                <Parameters>
                    <Order>1</Order>
                    <Cutoff>0.1</Cutoff>
                    <ZeroPhase>TRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU>                
            </Filter>
            <Filter>
                <Type>LowPass</Type>
                <Parameters>
                    <PassRipple>0.1</PassRipple>
                    <StopRipple>50</StopRipple>
                    <PassCutoff>1.5</PassCutoff>
                    <StopCutoff>2.5</StopCutoff>
                    <ZeroPhase>TRUE</ZeroPhase>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                </PMU> 
            </Filter>            
            <Filter>
                <Type>Median</Type>
                <Parameters>
                    <Order>5</Order>
                    <Endpoints>zeropad</Endpoints><!--Alternative is 'truncate'-->
                    <HandleNaN>includenan</HandleNaN><!--Alternative is 'omitnan'-->
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ComplexPower1</Name>
                    </Channel>
                    <Channel>
                        <Name>ApparentPower1</Name>
                    </Channel>
                     <Channel>
                        <Name>ActivePower1</Name>
                    </Channel>
                    <Channel>
                        <Name>ReactivePower1</Name>
                    </Channel>
                </PMU> 
            </Filter>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals2</MultiRatePMU>
                    <NewRate>5</NewRate>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                </PMU> 
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals3</MultiRatePMU>
                    <p>2</p>
                    <q>24</q>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU>
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals4</MultiRatePMU>
                    <q>12</q>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU>
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals5</MultiRatePMU>
                    <NewRate>120</NewRate>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU> 
            </Multirate>
            <Multirate>
                <Parameters>
                    <MultiRatePMU>Custom Signals6</MultiRatePMU>
                    <p>2</p>
                </Parameters>
                <PMU>
                    <Name>Custom Signals</Name>
                     <Channel>
                        <Name>ScaledAngleDiffSignal1</Name>
                    </Channel>
                    <Channel>
                        <Name>ScaledAngleDiffSignal2</Name>
                    </Channel>
                </PMU>
            </Multirate>
        </Stages>            
        <Wrap>
            <PMU>
                <Name>JDAY 500  A1 SA </Name>
            </PMU>
        </Wrap>
    </Processing>     
    <NameTypeUnit>
        <PMU>
            <Name>ALSN 500 A1 SA  </Name>
            <CurrentChannel>ALSN 500 A1 SA  .A500NAPAVINE_1MW</CurrentChannel>
            <NewChannel>ALSN new</NewChannel>
            <NewUnit>kV</NewUnit>
            <NewType>VMP</NewType>
        </PMU>
    </NameTypeUnit>
    <!--
    Specify the signals for output
    -->
    <SignalSelection>
        <PMU>
            <Name>ALSN 500 A1 SA  </Name>            
        </PMU>
        <PMU>
            <Name>JDAY 500  A1 SA </Name>            
        </PMU>
        <PMU>
            <Name>Custom Signals</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals2</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals3</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals4</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals5</Name>
        </PMU>
        <PMU>
            <Name>Custom Signals6</Name>
        </PMU>
    </SignalSelection>
</Configuration>